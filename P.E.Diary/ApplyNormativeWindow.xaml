<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:P.E.Diary"
        xmlns:Widgets="clr-namespace:P.E.Diary.Widgets" x:Class="P.E.Diary.ApplyNormativeWindow"
        mc:Ignorable="d"
        Title="Повести зачет" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="90"/>
            <ColumnDefinition Width="206*"/>
            <ColumnDefinition Width="149*"/>
        </Grid.ColumnDefinitions>
        <Widgets:NormativesApplicationLeftMenu x:Name="LeftMenu" HorizontalAlignment="Center" Width="90"/>
        <Widgets:NormativesList x:Name="NormativesList" Margin="0,0,5,0" Grid.Column="2"/>
        <Widgets:TestParticipatorsTable x:Name="Table" Grid.Column="1" Margin="0,0,1,0"/>

    </Grid>
</Window>
