<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:P.E.Diary"
        xmlns:Widgets="clr-namespace:P.E.Diary.Widgets" x:Class="P.E.Diary.MarksStatisticWindow"
        mc:Ignorable="d"
        Title="Оценки" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Widgets:PupilsMarksTable x:Name="MarksTable" Grid.Column="1"/>
        <Widgets:MarksLeftMenu x:Name="LeftMenu" HorizontalAlignment="Center" Width="90"/>

    </Grid>
</Window>
